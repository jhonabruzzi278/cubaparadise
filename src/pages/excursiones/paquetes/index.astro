---
import DefaultLayout from "../../../layouts/DefaultLayout.astro";
import HeaderStart from "../../../components/HeaderStart.astro";
import CatalogoCard from "../../../components/CatalogoCard.astro";
import { getCollection } from "astro:content";
import FeatureStar from "../../../components/FeatureStar.astro";

const excursionesPaquetes = await getCollection("excursiones-paquetes");

const page = "Paquetes Excursiones Cuba";
const image = "/img/viñales.jpg";
---

<DefaultLayout 
  title="Paquetes Excursiones Cuba - Viñales, Soroa, Cayos, Naturaleza"
  description="Paquetes excursiones aventura Cuba: Viñales + Soroa, Cayos + Trinidad, cascadas y naturaleza exuberante. Ecoturismo y aventura auténtica."
>
  <HeaderStart page={page} image={image} />
  <CatalogoCard titulo="Catálogo de Paquetes Excursiones" catalogo={excursionesPaquetes} />
  <FeatureStar />
</DefaultLayout>
