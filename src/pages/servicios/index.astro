---
import DefaultLayout from "../../layouts/DefaultLayout.astro";

import HeaderStart from "../../components/HeaderStart.astro";
import FeatureStar from "../../components/FeatureStar.astro";
import CatalogoCard from "../../components/CatalogoCard.astro";
import { getCollection } from "astro:content";

const servicios = await getCollection("servicios");
const catalogo = servicios;

const page = "Servicios Cuba";
const image = "/img/El_Capitolio_Havana_Cuba.jpg";
---

<DefaultLayout 
  title="Servicios Cuba - Renta Autos, Traslados, Asistencia Turística"
  description="Servicios turísticos Cuba: renta de autos, traslados aeropuerto, clases salsa, asistencia local. Todo para tu viaje perfecto a Cuba."
>
  <HeaderStart page={page} image={image} />
  <CatalogoCard titulo="Servicios Turísticos Cuba" catalogo={catalogo} />
  <FeatureStar />
</DefaultLayout>
